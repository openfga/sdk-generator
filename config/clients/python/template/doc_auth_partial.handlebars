# Defining the host is optional and defaults to api.{{{sampleApiDomain}}}
{{#each pathParams}}
{{#if @first}}
# Defining the store_id is required
{{/if}}
{{/each}}
# See configuration.py for a list of all supported configuration parameters.
configuration = {{{packageName}}}.Configuration(
    scheme = "https",
    host = "api.{{{sampleApiDomain}}}",
{{#each pathParams}}
{{#if @first}}
    store_id = 'YOUR_STORE_ID'
{{/if}}
{{/each}}
)

# In the case authenticate via API TOKEN
credentials = Credentials(method='api_token', configuration=CredentialConfiguration(api_token='TOKEN1'))
configuration = {{{packageName}}}.Configuration(
    scheme = "https",
    host = "api.{{{sampleApiDomain}}}",
{{#each pathParams}}
{{#if @first}}
    store_id = 'YOUR_STORE_ID',
{{/if}}
    credentials = credentials
{{/each}}
)
